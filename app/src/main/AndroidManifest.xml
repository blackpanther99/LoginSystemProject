<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.loginsystemproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StudentTeacher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".teacherRegister" />
        <activity android:name=".studentRegister" />
        <activity android:name=".teacherLogin" />
        <activity android:name=".studentLogin" />
        <activity android:name=".teacherRegistration" />
        <activity android:name=".studentRegistration"></activity>
    </application>

</manifest>